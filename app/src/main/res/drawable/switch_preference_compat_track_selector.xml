<?xml version="1.0" encoding="utf-8"?>

<!--
This selector style is used to tint the track of the SwitchPreferenceCompat component in the dark
mode.
-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/lowest_level_background_color" android:state_checked="true" />
    <item android:color="@color/lowest_level_background_color" android:state_selected="true" />
    <item android:color="@color/lowest_level_background_color" android:state_checked="false" />
    <item android:color="@color/lowest_level_background_color" android:state_selected="false" />
</selector>
